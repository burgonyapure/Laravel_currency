APP_NAME="app name"
APP_ENV=local
APP_KEY= #Don't forget to php artisan key:generate!
APP_DEBUG=true
APP_URL=appurl

LOG_CHANNEL=stack

#1st db connection 'authenticate' database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=authenticate
DB_USERNAME=user
DB_PASSWORD=pass

#2nd db connection 'bizonylatok' database
DB_CONNECTION_SECOND=mysql
DB_HOST_SECOND=127.0.0.1
DB_PORT_SECOND=3306
DB_DATABASE_SECOND=bizonylatok
DB_USERNAME_SECOND=user
DB_PASSWORD_SECOND=pass

#3rd db connection 'arfolyam' database
DB_CONNECTION_THIRD=mysql
DB_HOST_THIRD=127.0.0.1
DB_PORT_THIRD=3306
DB_DATABASE_THIRD=arfolyam
DB_USERNAME_THIRD=user
